\set QUIET 1
\set ON_ERROR_ROLLBACK interactive
\set VERBOSITY verbose
\set COMP_KEYWORD_CASE upper
\set PROMPT1 '%[%033[38;5;27m%]%`hostname -s`%[%033[%0m%]%@%[%033[0;32m%]%n%[%033[38;5;102m%]%|%/%[%033[31;5;27m%]%[%033[0m%] %# %x '
\set PROMPT2 '%R '
\set HISTFILE ~/.psql_history- :DBNAME
\set HISTCONTROL ignoredups
\pset null [null]
\pset pager always
\timing
\unset QUIET
